{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Backend",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "open_webui.main:app",
                "--port",
                "8080",
                "--host",
                "0.0.0.0",
                "--forwarded-allow-ips",
                "*",
                "--reload"
            ],
            "cwd": "${workspaceFolder}/backend",
            "console": "internalConsole"
        },
        {
            "name": "Frontend",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "preLaunchTask": "start-frontend",
            "postDebugTask": "terminate-frontend",
            
        },
        {
            "name": "Install (exe) Helper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/installer/raux_installer.py",
            "console": "integratedTerminal",
            "args": [
                "--install-dir",
                "${workspaceFolder}\\installer\\test_install"
            ]
        },
        {
            "name": "Installer (python)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/installer/ux_installer/install.py",
            "console": "integratedTerminal",
            "args": [
                "--yes"
            ]
        }
    ]
}